{
  "name": "quick-utils",
  "version": "0.1.1",
  "description": "quick-utils",
  "main": "dist/quick-utils.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest --coverage && opn coverage/index.html",
    "build": "webpack"
  },
