  "bugs": {
    "url": "https://github.com/mizne/quick-utils/issues"
  },
  "homepage": "https://github.com/mizne/quick-utils#readme",
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/node": "^8.0.57",
    "clean-webpack-plugin": "^0.1.17",
    "jest": "^21.2.1",
    "opn-cli": "^3.1.0",
    "ts-jest": "^21.2.3",
    "ts-loader": "^3.1.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.1",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "webpack": "^3.8.1"
