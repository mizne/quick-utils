    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "coverageReporters": [
      "text-summary",
      "html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mizne/quick-utils"
  },
